@{
    ViewData["Title"] = "Embeddings";
}

<h2>Embeddings Dashboard</h2>

<form asp-action="Generate" method="post">
    <label>Document ID:</label>
    <input type="text" name="documentId" class="form-control" />
    <button type="submit" class="btn btn-primary mt-2">Generate Embeddings</button>
</form>

@if (TempData["Message"] != null)
{
    <div class="alert alert-success mt-3">@TempData["Message"]</div>
}
